  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/react-apollo-graphqls/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/react-apollo-graphqls.git"
  },
  "keywords": [
    "react",
    "hoc",
    "apollo",
    "graphql"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.56",
    "@types/react": "^15.0.18",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-apollo": "^1.0.0-rc.3"
  },
  "devDependencies": {
    "rimraf": "^2.6.0",
    "typescript": "~3.0.3",
